	<body>
		{# loader Start #}
		<div id="loading">
			<div class="loader simple-loader">
				<div class="loader-body"></div>
			</div>
		</div>
		{# loader END #}

		{# sidebar START #}
		{% include 'sidebar.html.twig' %}
		{# sidebar END #}

		<main class="main-content">
			<div
				class="position-relative iq-banner">
				<!--Nav Start-->
				<nav class="nav navbar navbar-expand-lg navbar-light iq-navbar">
