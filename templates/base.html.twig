							href="/" class="navbar-brand">

							<!--Logo start-->
							<div class="logo-main">
								<div class="logo-normal">
									<img class="text-primary icon-30" src="{{ asset('images/mindsolutions/logo.svg') }}" alt="">
								</div>
								<div class="logo-mini">
									<img class="text-primary icon-30" src="{{ asset('images/mindsolutions/logo.svg') }}" alt="">
								</div>
							</div>
							<!--logo End-->

							<h4 class="logo-title">{{ site_name }}</h4>
						</a>
						<div class="sidebar-toggle" data-toggle="sidebar" data-active="true">
							<i class="icon">
								<svg width="20px" class="icon-20" viewbox="0 0 24 24">
									<path fill="currentColor" d="M4,11V13H16L10.5,18.5L11.92,19.92L19.84,12L11.92,4.08L10.5,5.5L16,11H4Z"/>
								</svg>
							</i>
						</div>
						{% if user is defined and user.getLicenseKey() is not null %}
							<div class="container-fluid search-container">
								<div class="row">
									<div class="input-group search-input col-md-6" style="padding: 0;">
										<span class="input-group-text" id="search-input">
											<svg class="icon-18" width="18" viewbox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
												<circle cx="11.7669" cy="11.7666" r="8.98856" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></circle>
												<path d="M18.0186 18.4851L21.5426 22" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"></path>
											</svg>
										</span>
										<input id="searchInput" type="search" class="form-control" placeholder="Search an event...">
									</div>
								</div>
								<div class="row">
									<div class="dropdown">
										<div
											id="searchResultsDropdown" class="dropdown-menu" aria-labelledby="search-input" style="left: 0; flex-direction: column;"><!-- Search results will be displayed here -->
										</div>
									</div>
								</div>
							</div>
						{% endif %}

						{% if solverBalance is defined %}
