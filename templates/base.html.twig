										<i class="fa fa-solid fa-globe" aria-hidden="true"></i>
										<span class="caret"></span>
										<span class="sr-only">{{ 'menu.choose_language'|trans }}</span>
									</a>
								</li>
								<li class="nav-item dropdown">
									<a class="py-0 nav-link d-flex align-items-center" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
										{% set avatar = (user.getImageUrl() is not null and user.getImageUrl() != "") ? user.getImageUrl() : asset('images/avatars/01.png') %}
										<img src="{{ avatar }}" alt="User-Profile" class="theme-color-default-img img-fluid avatar avatar-50 avatar-rounded">
										<div class="caption ms-3 d-none d-md-block ">
											{% set username = (user.getDiscordUsername() is not null and user.getDiscordUsername() != "") ? user.getDiscordUsername() : "Anonymous" %}
											<h6 class="mb-0 caption-title">{{ username }}</h6>
											<p class="mb-0 caption-sub-title">
												{% if is_granted('ROLE_MEMBER') %}
													Member
												{% elseif is_granted('ROLE_ADMIN') %}
													Admin
												{% else %}
													Guest
												{% endif %}
											</p>
										</div>
									</a>
									<ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">
