		rel="stylesheet" href="{{ asset('vendor/fullcalendar/list/main.css') }}"/>

		{# Webpack compiled files #}
		<script src="{{ asset('js/api.js') }}"></script>
		<script src="{{ asset('js/custom-functions.js') }}"></script>
		<script src="{{ asset('js/bs_tables.js') }}"></script>
		<script src="{{ asset('js/captcha.js') }}"></script>
		<script src="{{ asset('js/auth.js') }}"></script>
		<script src="{{ asset('js/dashboard.js') }}"></script>
		<script src="{{ asset('js/events.js') }}"></script>
		<script src="{{ asset('js/inventory.js') }}"></script>
		<script src="{{ asset('js/releases.js') }}"></script>
		<script src="{{ asset('js/settings.js') }}"></script>
		<script src="{{ asset('js/viagogo.js') }}"></script>
		<script src="{{ asset('js/listings.js') }}"></script>
