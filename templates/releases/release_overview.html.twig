{% extends 'base.html.twig' %}

{% block title %}Edit Release
{% endblock %}

{% block content %}

    <div class="conatiner-fluid content-inner py-0" style="margin-top: 40px;">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12">
					<div class="card">
						<div class="card-header d-flex justify-content-between">
							<div class="header-title">
								<h4 class="card-title">Edit {{ release.description }} release</h4>
							</div>
						</div>
						<div class="card-body">
							<div class="acc-edit">
								{{ form(editReleaseForm) }}
							</div>
						</div>
					</div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
